<launch>

    <rosparam file="low_level_control.yaml" command="load"/>

    <node name="robot_arm_controller_spawner" pkg="controller_manager" type="spawner"
          respawn="true" output="screen"
          args="/robopi/joint_state_controller
                /robopi/diff_drive_controller"/>

    <arg name="node_start_delay" default="5.0" />

    <node name="actuator_interface" pkg="robopi_hardware_interface" type="actuator_interface_node" output="screen"/>


</launch>




